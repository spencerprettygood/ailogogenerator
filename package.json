{
  "name": "ailogogenerator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint:report": "eslint --ext .js,.jsx,.ts,.tsx . --output-file eslint-report.json --format json",
    "typecheck": "tsc --noEmit",
    "type:watch": "tsc --noEmit --watch",
    "quality": "npm run lint && npm run typecheck",
    "quality:fix": "npm run lint:fix && npm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run '__tests__/.*\\.test\\.ts'",
    "test:integration": "vitest run 'app/api/.*\\.test\\.ts'",
    "test:e2e": "./test-runner.sh --all",
    "test:ci": "vitest run --coverage && tsc --noEmit",
    "prepare": "husky install || echo 'Skipping husky install'",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,js,jsx,json,css,scss}\"",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "npx serve docs/api",
    "docs:build": "npm run docs",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "optimize": "npm run format && npm run lint:fix && npm run typecheck && npm run test",
    "analyze:types": "node scripts/analyze-types.js",
    "analyze:unused": "node scripts/fix-unused.js",
    "analyze:performance": "node scripts/analyze-performance.js",
    "analyze:components": "node scripts/optimize-components.js",
    "analyze": "npm run analyze:types && npm run analyze:unused && npm run analyze:performance && npm run analyze:components"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@anthropic-ai/sdk": "^0.54.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.18.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.516.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "sharp": "^0.34.2",
    "svgo": "^3.3.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-dropzone": "^5.1.0",
    "@types/sharp": "^0.32.0",
    "@types/svgo": "^3.0.0",
    "@vitest/coverage-v8": "^1.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^4",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^5",
    "vitest": "^1.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0 || true",
      "prettier --write"
    ],
    "*.{json,css,scss,md,html}": [
      "prettier --write"
    ],
    "app/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0 || true"
    ],
    "components/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0 || true"
    ],
    "lib/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0 || true"
    ]
  },
  "typedoc": {
    "entryPoints": [
      "./lib",
      "./app",
      "./components"
    ],
    "entryPointStrategy": "expand",
    "out": "docs/api",
    "theme": "default",
    "excludePrivate": false,
    "excludeProtected": false,
    "excludeExternals": true,
    "includeVersion": true,
    "disableSources": false,
    "categorizeByGroup": true,
    "searchInComments": true
  }
}
